package com.example.demo.dto;

import java.util.HashSet;
import java.util.Set;

import com.example.demo.model.Book;

public class PublisherDTO {

	String name;
	String addressLine;
	String city;
	String state;
	int zip;
	Set<Book> books = new HashSet<>();
	
	public PublisherDTO(String name, String addressLine, String city, String state, int zip, Set<Book> books) {
		super();
		this.name = name;
		this.addressLine = addressLine;
		this.city = city;
		this.state = state;
		this.zip = zip;
		this.books = books;
	}
	
	
}
