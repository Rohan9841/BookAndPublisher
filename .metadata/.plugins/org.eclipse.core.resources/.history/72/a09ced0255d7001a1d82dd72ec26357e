package com.example.demo.dto;

import org.springframework.stereotype.Component;

@Component
public class PublisherDTO {

	String name;
	String addressLine;
	String city;
	String state;
	int zip;
	
	public PublisherDTO(String name, String addressLine, String city, String state, int zip) {
		super();
		this.name = name;
		this.addressLine = addressLine;
		this.city = city;
		this.state = state;
		this.zip = zip;
	}
	
	
}
